+package lesson1HW;
+
+public class Main {
+    public static void main(String[] args) {
+        searchMaxAndMin();
+    }
+
+    //1. Задать одномерный массив и найти в нём минимальный и максимальный элементы
+    private static void searchMaxAndMin() {
+        int[] arr = {20, 1, 5, 3, 11, 6, 8};
+        int max = arr[0];
+        int min = arr[0];
+        for (int i = 1; i < arr.length; i++) {
+            if (arr[i]>max){
+                max = arr[i];
+            } if (arr[i]<min){
+                min = arr[i];
+            }
+        }
+        System.out.println(max);
+        System.out.println(min);
+    }
+}
